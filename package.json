{
  "name": "about-me-automation",
  "version": "1.0.0",
  "description": "Enhanced Playwright test automation suite for arthursenko.com with comprehensive coverage",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "start": "npx playwright test --ui",
    "test:enhanced": "node scripts/test-runner.js",
    "test:headed": "node scripts/test-runner.js --headed",
    "test:debug": "node scripts/test-runner.js --debug",
    "test:ui": "node scripts/test-runner.js --ui",
    "test:performance": "npx playwright test tests/performance.spec.js",
    "test:accessibility": "npx playwright test tests/accessibility.spec.js",
    "test:security": "npx playwright test tests/security.spec.js",
    "test:cross-browser": "npx playwright test tests/cross-browser.spec.js"
  },
  "keywords": ["playwright", "testing", "automation", "accessibility", "performance", "security"],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@playwright/test": "^1.53.1",
    "axios": "^1.10.0"
  }
}
